<%= render 'shared/header', header: 'Log In' %>

<div class="well col-sm-6">
  <%= form_tag '/login' do %>
    <div class='form-group'>
      <%= label_tag :username %>
      <%= text_field_tag :username %>
    </div>
    <div class='form-group'>
      <%= label_tag :password %>
      <%= password_field_tag :password %>
    </div>
    <div class="form-group">
      <%= submit_tag 'Login', class: 'btn btn-info' %>
    </div>
  <% end %>
</div>
